{
  "name": "espol-ppl",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "npm run npm-scripts-info",
    "dev": "npm-run-all --parallel api:server client:server",
    "api:server": "cross-env NODE_ENV=development env-cmd .env nodemon app_api/server.api.js --config nodemon.json",
    "client:server": "cross-env NODE_ENV=development env-cmd .env node app_client/server.client.js",
    "client:server:test": "cross-env NODE_ENV=production:test env-cmd .env node app_client/server.client.js",
    "docs:api": "node node_modules/apidoc/bin/apidoc -e docs/ -i app_api/routes -o docs/api",
    "npm-scripts-info": "npm-scripts-info"
  },
  "scripts-info": {
    "api:server": "correr el api",
    "start": "ver todos los comandos"
  },
  "dependencies": {
    "body-parser": "~1.16.0",
    "cas-authentication": "^0.0.8",
    "co": "^4.6.0",
    "connect-mongo": "^2.0.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.15.5",
    "express-session": "^1.15.6",
    "knex": "^0.13.0",
    "mongodb": "^2.2.33",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0",
    "winston": "^3.0.0-rc0"
  },
  "devDependencies": {
    "cross-env": "^2.0.0",
    "env-cmd": "^7.0.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.1.1",
    "npm-scripts-info": "^0.3.6"
  }
}
