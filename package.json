{
  "name": "espol-ppl",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "npm run npm-scripts-info",
    "predev": "npm-run-all parcel parcel:estudiantes",
    "dev": "npm-run-all --parallel server sync watchify watchify:estudiantes",
    "debug:node": "cross-env NODE_ENV=debug env-cmd .env node --expose-gc --inspect=9222 app.js",
    "server": "cross-env NODE_ENV=development env-cmd .env nodemon server.js --config nodemon.json",
    "server:test": "cross-env NODE_ENV=testing PORT=8001 env-cmd .env nodemon server.js --config nodemon.json",
    "api:server": "cross-env NODE_ENV=development env-cmd .env nodemon app_api/server.api.js --config nodemon.json",
    "client:server": "cross-env NODE_ENV=development env-cmd .env node app_client/server.client.js",
    "client:server:cas": "cross-env NODE_ENV=cas env-cmd .env node app_client/server.client.js",
    "migrate:rollback": "cross-env NODE_ENV=development env-cmd .env  knex migrate:rollback --knexfile  databases/relational/knexfile.js",
    "migrate:latest": "cross-env NODE_ENV=development env-cmd .env  knex migrate:latest --knexfile  databases/relational/knexfile.js",
    "migrate:latest:testing": "cross-env NODE_ENV=testing env-cmd .env  knex migrate:latest --knexfile  databases/relational/knexfile.js",
    "seed:run": "cross-env NODE_ENV=development env-cmd .env  knex seed:run --knexfile  databases/relational/knexfile.js",
    "seed:run:testing": "cross-env NODE_ENV=testing env-cmd .env knex seed:run --knexfile  databases/relational/knexfile.js",
    "predevelopment": "cross-env NODE_ENV=development env-cmd .env node scripts/database/setup_database_mysql.js",
    "development": "npm-run-all migrate:latest seed:run",
    "pretest": "cross-env NODE_ENV=testing env-cmd .env node scripts/database/setup_database_mysql.js",
    "test": "npm-run-all migrate:latest:testing seed:run:testing test:integration",
    "test:integration": "yarn cross-env NODE_ENV=testing env-cmd .env nyc --reporter=html mocha --opts app_api/test/integration/mocha.opts app_api/test/integration/**/*.test.js --exit",
    "test:integration:watch": "yarn cross-env NODE_ENV=testing env-cmd .env  mocha --opts app_api/test/integration/mocha.opts app_api/test/**/**/*.test.js --watch --recursive",
    "sync": "browser-sync start --proxy 'http://localhost:8000' --ws --port 5000 --no-open --no-ui",
    "docs:api": "node node_modules/apidoc/bin/apidoc -e docs/ -i app_api/routes -o docs/api",
    "predocs:code": "gitbook install ./docs/code",
    "docs:code": "gitbook build ./docs/code",
    "docs:code:s": "gitbook serve ./docs/code",
    "docs": "npm-run-all docs:api docs:code",
    "npm-scripts-info": "npm-scripts-info",
    "browser-sync": "browser-sync",
    "watchify": "watchify -vd -p [ browserify-hmr  -p 8888 -u localhost:8888 ] -e app_client/profesores/src/main.js -o app_client/profesores/dist/build.js",
    "watchify:estudiantes": "watchify -vd -p [ browserify-hmr -p 8887 -u localhost:8887 ] -e app_client/estudiantes/src/main.js -o app_client/estudiantes/dist/build.js",
    "parcel": "browserify  -e app_client/profesores/src/main.js -p [ parcelify -o app_client/profesores/dist/build.css] -o public/tmp",
    "parcel:estudiantes": "browserify  -e app_client/estudiantes/src/main.js -p [ parcelify -o app_client/estudiantes/dist/build.css] -o public/tmp",
    "minify:estudiantes": "minify --output app_client/estudiantes/dist/build.css app_client/estudiantes/dist/build.css",
    "minify:profesores": "minify --output app_client/profesores/dist/build.css app_client/profesores/dist/build.css",
    "production": "npm-run-all js:production:profesores js:production:estudiantes",
    "postproduction": "npm-run-all minify:profesores minify:estudiantes",
    "js:production:profesores": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o app_client/profesores/dist/build-component.css ] -e app_client/profesores/src/main.js | uglifyjs -c drop_console=true,warnings=false -m > app_client/profesores/dist/build.js",
    "js:production:estudiantes": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o app_client/estudiantes/dist/build-component.css ] -e app_client/estudiantes/src/main.js | uglifyjs -c drop_console=true,warnings=false -m > app_client/estudiantes/dist/build.js",
    "browserify": "browserify",
    "cypress": "./node_modules/.bin/cypress open --project ./app_client"
  },
  "scripts-info": {
    "start": "Ver informacion de los scripts",
    "development": "Crea la base de datos development (borra la existente)",
    "dev": "Empieza la aplicacion",
    "production": "build estudiantes y profesor cliente apps",
    "debug:node": "Habilita el modo debug para chrome",
    "cas": "Para realizar pruebas del cas",
    "test": "Corre los test automaticos",
    "docs": "Genera la documentacion, /docs/api y /docs/code",
    "migrate:latest": "Correr las migraciones en development",
    "seed:run": "Corre los seed en development"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "style": [
    "./node_modules/vue-material/dist/vue-material.css"
  ],
  "nyc": {
    "report-dir": "./app_client/varios/coverage",
    "exclude": [
      "app_api/test/**/**/*.test.js"
    ]
  },
  "dependencies": {
    "body-parser": "~1.16.0",
    "cas-authentication": "^0.0.8",
    "co": "^4.6.0",
    "connect-mongo": "^2.0.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.15.5",
    "express-session": "^1.15.6",
    "knex": "^0.13.0",
    "mongodb": "^2.2.33",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0",
    "winston": "^3.0.0-rc0"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "axios": "^0.17.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.13",
    "browserify-hmr": "^0.3.5",
    "chai": "^4.1.2",
    "chalk": "^2.2.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "cypress": "^1.0.2",
    "env-cmd": "^7.0.0",
    "envify": "^4.1.0",
    "eslint": "^3.3.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-html": "^1.5.2",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "faker": "^4.1.0",
    "gitbook-cli": "^2.3.2",
    "istanbul": "^0.4.5",
    "leakage": "^0.3.0",
    "minifier": "^0.8.1",
    "mocha": "^4.0.1",
    "mochawesome": "^2.3.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.1.1",
    "npm-scripts-info": "^0.3.6",
    "nyc": "^11.2.1",
    "offline-js": "^0.7.19",
    "parcelify": "^2.2.0",
    "proxyquireify": "^3.0.1",
    "sinon": "^4.0.1",
    "socket.io-client": "^2.0.3",
    "supertest": "^3.0.0",
    "testdouble": "^3.2.6",
    "uglify-es": "^3.1.5",
    "vue": "^2.5.2",
    "vue-material": "^0.7.5",
    "vue-router": "^2.7.0",
    "vue-socket.io": "^2.1.1-a",
    "vueify": "^9.4.1",
    "vuetify": "^0.14.1",
    "vuex": "^2.3.1",
    "watchify": "^3.9.0"
  },
  "apidoc": {
    "name": "apidoc-ppl",
    "version": "2.0.0",
    "description": "Documentacion Api PPL",
    "title": "Documentacion Api PPL",
    "url": "",
    "sampleUrl": "http://localhost:8000/api",
    "template": {
      "withCompare": true,
      "withGenerator": true
    }
  }
}
